{
    "swagger": "2.0",
    "info": {
        "title": "AkulakuAstra",
        "contact": {
            "email": "faujiakbar@gmail.com"
        },
        "version": "1.0.0"
    },
    "basePath": "/",
    "schemes": [
        "http",
        "https"
    ],
    "paths": {
        "/auth": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Authentication",
                "description": "Login Lendtick",
                "operationId": "auth",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "username",
                        "in": "formData",
                        "description": "Email for login Lendtick",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "formData",
                        "description": "Password bond to that email",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful"
                    }
                }
            }
        },
        "/auth/check": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Check Authentication",
                "description": "Check Token Lendtick",
                "operationId": "checkAuth",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "Token Authorization",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful"
                    }
                }
            }
        },
        "/auth/refresh": {
            "get": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Refresh Authentication",
                "description": "Refresh Token Lendtick",
                "operationId": "refreshAuth",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "Token Authorization",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful"
                    }
                }
            }
        },
        "/reg": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Registration",
                "description": "Register Lendtick",
                "operationId": "reg",
                "consumes": [
                    "application/x-www-form-urlencoded"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "name",
                        "in": "formData",
                        "description": "Name",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "company",
                        "in": "formData",
                        "description": "Company code from list",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "identity_photo",
                        "in": "formData",
                        "description": "Base64 jpg of identity",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "company_identity_photo",
                        "in": "formData",
                        "description": "Base64 jpg of company identity",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "phone_number",
                        "in": "formData",
                        "description": "Phone Number",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "formData",
                        "description": "Email new user",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "personal_photo",
                        "in": "formData",
                        "description": "Base64 jpg of self photo",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful"
                    }
                }
            }
        }
    },
    "definitions": {}
}